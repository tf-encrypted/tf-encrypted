sleep 1
TFE_NODE_NAME=weights_provider TFE_SERVER0_ADDRESS=localhost:4441 TFE_SERVER1_ADDRESS=localhost:4442 TFE_CRYPTO_PRODUCER_ADDRESS=localhost:4443 TFE_WEIGHT_PROVIDER_ADDRESS=localhost:4444 $DIRECTORY/serve &
